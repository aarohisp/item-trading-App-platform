{
    "name": "atrs_fe",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "start": "react-app-rewired start",
        "dev": "react-app-rewired --openssl-legacy-provider start",
        "build": "react-app-rewired build",
        "test": "react-app-rewired --env=jsdom",
        "less": "lessc --js src/styles/wieldy.less public/css/style.css && lessc --js src/styles/wieldy.less public/css/style.min.css --compress -x",
        "format:check": "prettier --check ./src/*",
        "format:write": "prettier --write ./src/*",
        "lint:check": "eslint --no-error-on-unmatched-pattern .",
        "lint:fix": "eslint --no-error-on-unmatched-pattern --fix ."
    },
    "eslintConfig": {
        "extends": "react-app",
        "rules": {
            "no-console": "warn",
            "no-self-compare": "error",
            "no-useless-return": "error",
            "no-mixed-spaces-and-tabs": "error",
            "max-len": [
                "error",
                {
                    "code": 500,
                    "tabWidth": 2,
                    "comments": 120
                }
            ]
        }
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "dependencies": {
        "@ant-design/compatible": "^1.0.8",
        "@ant-design/icons": "^4.3.0",
        "@babel/compat-data": "^7.12.7",
        "@testing-library/jest-dom": "^5.11.9",
        "@testing-library/react": "^11.2.3",
        "@testing-library/user-event": "^12.6.0",
        "@types/react-custom-scrollbars": "^4.0.10",
        "@types/url-search-params": "^1.1.0",
        "antd": "^4.15.1",
        "axios": "^0.21.1",
        "connected-react-router": "^6.8.0",
        "dotenv": "^16.0.3",
        "file-saver": "^2.0.5",
        "less": "^4.1.0",
        "less-loader": "5.0.0",
        "lodash": "^4.17.20",
        "moment-timezone": "^0.5.43",
        "pdfmake": "^0.2.7",
        "react": "^17.0.1",
        "react-big-calendar": "^0.30.0",
        "react-custom-scrollbars": "^4.2.1",
        "react-dom": "^17.0.1",
        "react-intl": "^5.10.16",
        "react-notifications": "^1.7.2",
        "react-redux": "^7.2.2",
        "react-router-dom": "^5.2.0",
        "react-scripts": "^4.0.1",
        "react-stickies": "^0.0.16",
        "redux": "^4.0.5",
        "redux-thunk": "^2.3.0",
        "to-words": "^3.5.0",
        "typescript": "4.4.4",
        "url-search-params": "^1.1.0"
    },
    "devDependencies": {
        "@types/file-saver": "^2.0.5",
        "@types/pdfmake": "^0.2.2",
        "@types/react-dom": "^17.0.1",
        "@types/react-router-dom": "^5.2.0",
        "babel-plugin-import": "^1.13.0",
        "customize-cra": "^0.9.1",
        "prettier": "^2.8.8",
        "react-app-rewired": "^2.1.5",
        "redux-devtools-extension": "^2.13.9"
    }
}
